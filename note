                direction : 'vertical',//纵向
                onlyExternal : true,//不可touch和mouse
                autoplay : 2000,//自动播放
                loop : true,//循环
                // slideClass:"slideClass1"
            });

var number = $("#phone").val();
if(!(/^1(3|4|5|7|8)\d{9}$/.test(number))){console.log("请输入正确的手机号!");return;};

$("#phone").on("input",function(){
    _self.limitNum($(this)[0]);
});

